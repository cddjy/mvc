package java8.base;

public class Grammar {

	public static void main(String[] args) {
//		HandleInterface handleInterface = new  HandleInterface((a,b)->a*b);
		// 扩展方法 default method	
		HandleInterface handleInterface = new  HandleInterface(){
			@Override
			public int handle(int a, int b) {
				return a*b;
			}
			
		};

	}

}
